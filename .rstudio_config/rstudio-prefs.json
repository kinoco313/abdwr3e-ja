{
    "restore_source_documents": false,
    "wrap_tab_navigation": false,
    "save_workspace": "never",
    "load_workspace": false,
    "initial_working_directory": "~/workspace",
    "always_save_history": false,
    "restore_last_project": false,
    "reuse_sessions_for_project_links": true,
    "auto_detect_indentation": true,
    "insert_native_pipe_operator": true,
    "soft_wrap_r_files": true,
    "highlight_selected_line": true,
    "highlight_r_function_calls": true,
    "rainbow_parentheses": true,
    "line_ending_conversion": "passthrough",
    "default_encoding": "UTF-8",
    "syntax_color_console": true,
    "help_font_size_points": 12,
    "font_size_points": 11,
    "editor_theme": "Tomorrow Night 80s",
    "server_editor_font_enabled": true,
    "server_editor_font": "DejaVu Sans Mono",
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Presentation"
        ],
        "tabSet2": [
            "Environment",
            "History",
            "Files",
            "Plots",
            "Connections",
            "Packages",
            "Help",
            "Build",
            "VCS",
            "Tutorial",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "cran_mirror": {
        "name": "Japan (Yonezawa) [https]",
        "host": "Yamagata University",
        "url": "https://ftp.yz.yamagata-u.ac.jp/pub/cran/",
        "country": "jp",
        "ok": 1,
        "secondary": ""
    },
    "rmd_viewer_type": "pane",
    "real_time_spellchecking": false,
    "posix_terminal_shell": "custom",
    "custom_shell_command": "/usr/bin/bash",
    "default_project_location": "~",
    "source_with_echo": true,
    "show_hidden_files": true,
    "jobs_tab_visibility": "shown",
    "remove_history_duplicates": true
}